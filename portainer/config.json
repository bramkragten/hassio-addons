{
  "name": "Portainer",
  "version": "latest",
  "slug": "portainer",
  "image": "portainer/portainer",
  "description": "Portainer addon.",
  "url": "https://hub.docker.com/r/portainer/portainer/",
  "startup": "initialize",
  "privileged": ["SYS_ADMIN", "NET_ADMIN", "SYS_RAWIO"]
  "boot": "auto",
  "host_network": true,
  "webui": "http://[HOST]:[PORT:9000]/",
  "ports": {
    "9000/tcp": 9000
  },
  "environment": {
    "GF_PATHS_DATA": "/data",
    "GF_PATHS_PLUGINS": "/data/plugins"
  },
  "options": {

  },
  "schema": {
  }
}